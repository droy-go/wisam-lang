# مثال: الحلقات

# حلقة لكل
اكتب "=== حلقة لكل ==="
لكل عدد من 1 إلى 10
    اكتب "العدد: {عدد}"
انتهى

# حلقة طالما
اكتب ""
اكتب "=== حلقة طالما ==="
ليكن رقم = 5

طالما رقم > 0
    اكتب "العدد التنازلي: {رقم}"
    رقم = رقم - 1
انتهى

# جدول الضرب
اكتب ""
اكتب "=== جدول الضرب ==="

لكل س من 1 إلى 5
    لكل ص من 1 إلى 5
        ليكن ناتج = س * ص
        اكتب "{س} × {ص} = {ناتج}"
    انتهى
    اكتب "---"
انتهى
